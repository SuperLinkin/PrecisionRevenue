{
  "name": "precision-revenue",
  "version": "1.0.0",
  "description": "Precision Revenue API Server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev:server": "ts-node-dev --respawn server/index.ts",
    "dev:client": "vite --config vite.config.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "tsc && npm run build:client",
    "build:client": "cd client && vite build",
    "test": "jest",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:init": "ts-node server/init-db.ts",
    "db:migrate": "ts-node server/migrations.ts",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview"
  },
  "dependencies": {
    "@langchain/openai": "^0.0.14",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-toast": "^1.2.11",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.74.4",
    "@xenova/transformers": "^2.17.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "framer-motion": "^12.9.2",
    "langchain": "^0.1.37",
    "lucide-react": "^0.503.0",
    "memorystore": "^1.6.7",
    "openai": "^4.96.0",
    "pdf-parse": "^1.1.1",
    "pgvector": "^0.1.8",
    "postcss": "^8.5.3",
    "postgres": "^3.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.4",
    "wouter": "^3.7.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vercel/node": "^5.1.14",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.31.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vite": "^5.0.0"
  }
}
